{
  "name": "gradebox-backend",
  "version": "1.0.0",
  "description": "Backend API for GradeBox",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "migrate": "ts-node src/migrations/run-migrations.ts"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "mssql": "^10.0.2",
    "patch-package": "^8.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/mssql": "^9.1.5",
    "@types/node": "^20.12.7",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.4.5"
  }
}
